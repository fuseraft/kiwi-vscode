{
    "Struct Definition": {
        "prefix": "struct",
        "body": [
            "struct $1",
            "  fn new()",
            "    @$2 = $3",
            "  end",
            "  ",
            "  fn $4($5)",
            "    $6",
            "  end",
            "  ",
            "  fn $7()",
            "    return @$2",
            "  end",
            "end"
        ],
        "description": "Defines a new struct with initial methods."
    },
    "Main Function": {
        "prefix": "main",
        "body": [
            "fn main()",
            "  $1",
            "end",
            "",
            "main()"
        ],
        "description": "Defines the main function and calls it."
    },
    "Function Definition": {
        "prefix": "fn",
        "body": [
            "fn $1()",
            "  $2",
            "end"
        ],
        "description": "Creates a new function definition."
    },
    "Module Definition": {
        "prefix": "package",
        "body": [
            "package $1",
            "  $0",
            "end"
        ],
        "description": "Defines a new package."
    },
    "Import Module": {
        "prefix": "import",
        "body": [
            "import \"@$1\" as $2"
        ],
        "description": "Imports a module and assigns it an alias."
    },
    "Try-Catch Block": {
        "prefix": "trycatch",
        "body": [
            "try",
            "  $1",
            "catch ($2)",
            "  $3",
            "end"
        ],
        "description": "Creates a try-catch block for error handling."
    },
    "Try-Catch-Finally Block": {
        "prefix": "trycatchfinally",
        "body": [
            "try",
            "  $1",
            "catch ($2)",
            "  $3",
            "finally",
            "  $4",
            "end"
        ],
        "description": "Creates a try-catch-finally block for error handling."
    },
    "For Loop": {
        "prefix": "forloop",
        "body": [
            "for $1 in $2 do",
            "  $3",
            "end"
        ],
        "description": "Creates a for loop that iterates over a range or collection."
    },
    "While Loop": {
        "prefix": "whileloop",
        "body": [
            "while $1 do",
            "  $2",
            "end"
        ],
        "description": "Creates a while loop that runs while a condition is truthy."
    },
    "Repeat Loop": {
        "prefix": "repeatloop",
        "body": [
            "repeat $1 as $2 do",
            "  $3",
            "end"
        ],
        "description": "Creates a repeat loop that iterates n number of times."
    },
    "Lambda Block": {
        "prefix": "with",
        "body": [
            "with ($1) do",
            "  $2",
            "end"
        ],
        "description": "Creates a lambda block."
    },
    "Conditional Block": {
        "prefix": "if",
        "body": [
            "if $1",
            "  $0",
            "end"
        ],
        "description": "Creates an if statement."
    },
    "If-Else Block": {
        "prefix": "ifelse",
        "body": [
            "if $1",
            "  $2",
            "else",
            "  $3",
            "end"
        ],
        "description": "Creates an if-else statement."
    },
    "If-Elsif-Else Block": {
        "prefix": "ifelse",
        "body": [
            "if $1",
            "  $2",
            "elsif $3",
            "  $4",
            "else",
            "  $5",
            "end"
        ],
        "description": "Creates an if-elsif-else statement."
    },
    "Case Block": {
        "prefix": "case",
        "body": [
            "case $1 as $2",
            "  when $3 $4",
            "  when $5 $6",
            "  else $7",
            "end"
        ],
        "description": "Creates a case statement."
    },
    "Do Block": {
        "prefix": "do",
        "body": [
            "do",
            "  $0",
            "end"
        ],
        "description": "Creates a do block."
    }
}
