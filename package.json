{
  "name": "kiwi-lang",
  "displayName": "kiwi-lang",
  "description": "Syntax highlighting for the Kiwi programming language",
  "version": "0.0.22",
  "publisher": "fuseraft",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Programming Languages", 
    "Snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fuseraft/kiwi-vscode.git"
  },
  "contributes": {
    "languages": [
      {
        "id": "kiwi",
        "aliases": [
          "Kiwi",
          "kiwi",
          "kiwi-lang",
          "kiwi-language"
        ],
        "extensions": [
          ".kiwi",
          ".ü•ù"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "kiwi",
        "scopeName": "source.kiwi",
        "path": "./syntaxes/kiwi.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "kiwi",
        "path": "./snippets/kiwi-snippets.json"
      }
    ],
    "iconThemes": [
      {
        "id": "kiwi-icon-theme",
        "label": "Kiwi Icon Theme",
        "path": "./file-icons/kiwi-icon-theme.json"
      }
    ]
  }
}
